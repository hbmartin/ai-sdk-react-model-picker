{
  "name": "ai-sdk-react-model-picker",
  "private": false,
  "version": "0.2.0",
  "description": "A flexible, theme-aware React component library for selecting and managing AI models. Built for seamless integration with Vercel's AI SDK v5, with special support for VSCode extensions and JetBrains IDEs.",
  "homepage": "https://github.com/hbmartin/ai-sdk-react-model-picker",
  "bugs": "https://github.com/hbmartin/ai-sdk-react-model-picker/issues",
  "keywords": [
    "ai",
    "ai-sdk",
    "react",
    "react-component",
    "react-library",
    "typescript",
    "controlled-components",
    "component-library",
    "vercel-ai-sdk",
    "ai-models",
    "model-selection",
    "vscode-extension",
    "ide-integration",
    "theme-aware",
    "plugin-architecture",
    "type-safe",
    "frontend",
    "ui-components"
  ],
  "type": "module",
  "author": {
    "name": "Harold Martin",
    "email": "harold.martin@gmail.com",
    "url": "https://www.linkedin.com/in/harold-martin-98526971/"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hbmartin/ai-sdk-react-model-picker"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css",
    "./tailwind-preset": {
      "import": "./tailwind-preset.js",
      "require": "./tailwind-preset.cjs"
    }
  },
  "files": [
    "dist",
    "tailwind-preset.js",
    "tailwind-preset.cjs"
  ],
  "sideEffects": [
    "**/*.css",
    "**/styles/**"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "tsc --noEmit && eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "preview": "vite preview",
    "prepublishOnly": "pnpm run build",
    "clean": "rm -rf dist node_modules/.tmp",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "pnpm run build && storybook build"
  },
  "engines": {
    "node": ">=20"
  },
  "peerDependencies": {
    "@ai-sdk/amazon-bedrock": ">=2.0.0",
    "@ai-sdk/anthropic": ">=2.0.0",
    "@ai-sdk/azure": ">=2.0.0",
    "@ai-sdk/cohere": ">=2.0.0",
    "@ai-sdk/deepseek": ">=1.0.0",
    "@ai-sdk/google": ">=2.0.0",
    "@ai-sdk/mistral": ">=2.0.0",
    "@ai-sdk/openai": ">=2.0.0",
    "@openrouter/ai-sdk-provider": ">=1.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-hook-form": "^7.62.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/provider": {
