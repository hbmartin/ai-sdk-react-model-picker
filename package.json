    },
    "@ai-sdk/azure": {
      "optional": true
    },
    "@ai-sdk/mistral": {
      "optional": true
    },
    "@ai-sdk/cohere": {
      "optional": true
    },
    "@ai-sdk/amazon-bedrock": {
      "optional": true
    },
    "@openrouter/ai-sdk-provider": {
      "optional": true
    },
    "@ai-sdk/deepseek": {
      "optional": true
    }
  },
